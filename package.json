{
  "private": true,
  "name": "keith-component",
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "npm run build-lib && webpack && node ./scripts/gen_less_entry.js",
    "build-lib": "cross-env BUILD_TYPE=lib father-build && cross-env BUILD_TYPE=es father-build",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build",
    "deploy": "cross-env SITE_DEPLOY='TRUE' npm run site && gh-pages -d ./dist",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx  --fix --format=pretty ./packages && npm run prettier",
    "lint:prettier": "npm run prettier &&  git diff && prettier --version && prettier --check \"packages/**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --loglevel warn",
    "site": "node ./scripts/preDeploy && dumi build",
    "size": "npm run build && size-limit",
    "size:build": "cross-env PRO_COMPONENTS_CI='CI' webpack",
    "release": "node ./scripts/release.js"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "fabric verify-commit"
  },
  "lint-staged": {
    "packages/**/*.{js,ts,jsx,tsx, md, json}": [
      "eslint --fix",
      "prettier --write",
      "git add ."
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.33.0",
    "@ant-design/pro-skeleton": "^1.0.5",
    "@umijs/plugin-antd": "^0.15.0",
    "@umijs/ssr-darkreader": "^4.9.44",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "antd": "^4.18.9",
    "moment": "^2.29.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.2.3",
    "react-lazyload": "^3.2.0",
    "umi": "^3.5.22"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@size-limit/preset-big-lib": "^7.0.8",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.3",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.2.10",
    "@umijs/yorkie": "^2.0.5",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.12.2",
    "babel-types": "^6.26.0",
    "chalk": "^5.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dumi": "^1.1.39",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "jest": "^27.0.3",
    "jest-axe": "^6.0.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-environment-node": "^27.4.4",
    "jest-fetch-mock": "^3.0.3",
    "lerna": "^4.0.0",
    "lerna-changelog": "^2.2.0",
    "less-loader": "^9.0.0",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^1.6.0",
    "mockdate": "^3.0.5",
    "moment-timezone": "^0.5.34",
    "prettier": "^2.2.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "size-limit": "^7.0.8",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.7.0",
    "write-pkg": "^4.0.0",
    "yorkie": "^2.0.0"
  },
  "size-limit": [
    {
      "path": "packages/button/dist/**/*.js",
      "limit": "100 s",
      "webpack": false,
      "running": false
    }
  ]
}
